	$(document).ready(function(){
		$(".shrinkBtn").click(function(){
			if("block" == $(".select_div").css("display")){
				$(".select_div").hide();
			}else{
				$(".select_div").show();
			}
		});
		if(getPathArr&&parent&&parent.navigateToMenu){
			var path = getPathArr();
			if(path && path.length>=3){
				parent.navigateToMenu(path[0], path[1], path[2]);
			}
		}
	});
	</script>
