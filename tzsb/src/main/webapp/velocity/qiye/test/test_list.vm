	info("第"+hang+"行，第"+lie+"列");
}
var rowCount = $n("chk").length;
function addRow(){
	rowCount ++;
	var htmlArr = [
			"<input type='checkbox' name='chk'>",
			"<span name='xuhao'>" + ($n("chk").length+1) + "</span>",
			(1000+$n("chk").length),
