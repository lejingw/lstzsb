package org.konghao.student.ws;

import java.util.List;

import javax.jws.WebMethod;
import javax.jws.WebParam;
import javax.jws.WebService;

import org.konghao.student.model.RewardPunish;
import org.konghao.student.model.Student;

@WebService
public interface IMyService {
	/**
     * 
     * @param xh
     * @return
     *     returns org.konghao.student.ws.Student
     */
    @WebMethod
    public Student getStudent(
        @WebParam(name = "xh", targetNamespace = "http://ws.student.konghao.org")
        String xh);

    /**
     * 
     * @param xh
     * @return
     *     returns java.util.List<org.konghao.student.ws.RewardPunish>
     */
    @WebMethod
    public List<RewardPunish> listRpByStu(
        @WebParam(name = "xh", targetNamespace = "http://ws.student.konghao.org")
        String xh);

    /**
     * 
     * @param year
     * @param semester
     * @return
     *     returns java.util.List<org.konghao.student.ws.Student>
     */
    @WebMethod
    public List<Student> listRewardStu(
        @WebParam(name = "year", targetNamespace = "http://ws.student.konghao.org")
        int year,
        @WebParam(name = "semester", targetNamespace = "http://ws.student.konghao.org")
        String semester);

    /**
     * 
     * @param year
     * @param semester
     * @return
     *     returns java.util.List<org.konghao.student.ws.Student>
     */
    @WebMethod
    public List<Student> listPunishStu(
        @WebParam(name = "year", targetNamespace = "http://ws.student.konghao.org")
        int year,
        @WebParam(name = "semester", targetNamespace = "http://ws.student.konghao.org")
        String semester);
}
